<?xml version="1.0" encoding="UTF-8"?>
<mustache>
  <script id="login_tpl" type="text/template">
		<div class="row">
      <div class="col-md-3 col-centered" id="login_container">
      	<div class="panel panel-default animated fadeInUp">
					<div class="panel-heading">
						Identification
					</div>
					<div class="panel-body">
						<div class="input-group">
							<span class="input-group-addon glyphicon glyphicon-user"> </span>
							<input type="text" id="identifiant" name="identifiant_rlm" class="form-control" placeholder="Identifiant" />
						</div>
						<br />
						<div class="input-group">
							<span class="input-group-addon glyphicon glyphicon-asterisk"> </span>
							<input type="password" id="password" name="password_rlm" class="form-control" placeholder="Mot de passe" />
						</div>
						<br />
						<button type="submit" class="btn btn-success form-control" id="login_try">
							 Valider
						</button>
					</div>
				</div>
			</div>
		</div>
	</script>
</mustache>